{
    "name": "{{cookiecutter.app}}",
    "services": [{
        "name": "items",
        "status": "enable",
        "storage": {
            "model": "{{cookiecutter.app}}.services.items.model.Items",
            "connector": "{{cookiecutter.storage}}"
        },
        "autoload": {
            "routes": {
                "/": {
                   "POST": "create"
                }
            }
        }
    }],
    "storages": {
        "default": "{{cookiecutter.storage}}",
        "connectors": [{
                "name": "{{cookiecutter.storage}}",
                "factory": "sowba.storage.rocks_db.RocksDBStorage",
                "configuration": {},
                "settings": {}
            },
            {
                "name": "mongodb",
                "factory": null,
                "configuration": {},
                "settings": {}
            },
            {
                "name": "couchbase",
                "factory": null,
                "configuration": {},
                "settings": {}
            }
        ]
    },
    "asgi": {
        "host": "0.0.0.0",
        "port": 8000,
        "debug": false,
        "reload": false,
        "timeout_keep_alive": 20,
        "proxy_headers": true,
        "log_level": "info"
    },
    "cors": {
        "allow_origins": [
            "*"
        ],
        "allow_headers": [
            "*"
        ],
        "allow_methods": [
            "*"
        ]
    }
}